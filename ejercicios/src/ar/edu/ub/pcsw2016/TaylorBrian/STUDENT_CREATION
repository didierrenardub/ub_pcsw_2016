import ar.edu.ub.pcsw2016.IExercise;
import ar.edu.ub.pcsw2016.TaylorBrian.CStudentTaylorBrian;
public class STUDENT_CREATION implements IExercise{
    public float grade;
    public ID id(){
        return IExercise.ID.STUDENT_CREATION;
    }
    private void run(){
        system.out.println("Name: ", CStudentTaylorBrian.name, "\n Registration Number: ",
                CStudentTaylorBrian.registrationNumber, "\n Grade Average: ", CStudentTaylorBrian.gradesAverage);
    }
    public float grade(){
        return grade;
    }

    public void setGrade(grade){
        this.setGrade==grade;
    }

}